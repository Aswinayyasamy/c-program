#include <stdio.h>
#include <math.h>

int main() {
    float num;
    
    
    scanf("%f", &num);
    
    
    printf("%.5f\n", num);
    printf("%.4f\n", num);
    printf("%.3f\n", num);
    printf("%.2f\n", num);
    printf("%.1f\n", num);
    printf("%.0f\n", round(num));  // round to nearest integer for 0 decimal places

    return 0;
}

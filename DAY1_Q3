#include <stdio.h>
#include <string.h>

int main() {
    char str[1000];

    // Read input string
    scanf("%s", str);

    // Get the length of the string
    int len = strlen(str);

    // Print characters in reverse order
    for(int i = len - 1; i >= 0; i--) {
        printf("%c", str[i]);
    }

    printf("\n");
    return 0;
}

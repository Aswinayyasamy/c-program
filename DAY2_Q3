#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char name;
    int arrears;
    float cgpa;
    scanf("%s %d %f", name, &arrears, &cgpa);
    if ((arrears == 1 && cgpa > 70) || ((arrears == 1 || arrears == 2) && cgpa > 75)) {
        printf("Eligible for Placement");
    } else {
        printf("Not Eligible for Placement");
    }

    return 0;
}


#include <stdio.h>

int main() {
    int num1, num2, num3;

    
    scanf("%d", &num1);
    scanf("%d", &num2);
    scanf("%d", &num3);

    
    if (num1 >= num2 && num1 >= num3) {
        printf("Number1 is maximum with value of %.1f\n", (float)num1);
    } else if (num2 >= num1 && num2 >= num3) {
        printf("Number2 is maximum with value of %.1f\n", (float)num2);
    } else {
        printf("Number3 is maximum with value of %.1f\n", (float)num3);
    }

    return 0;
}

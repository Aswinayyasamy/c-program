#include <stdio.h>

int main() {
    int rows, cols, tree_num;
    scanf("%d", &rows);
    scanf("%d", &cols);
    scanf("%d", &tree_num);

    int column = (tree_num - 1) / rows + 1;

    if (column == 2 || column == (cols - 1)) {
        printf("It is a mango tree\n");
    } else {
        printf("It is not a mango tree\n");
    }

    return 0;
}
